CREATE TABLE Notifications (
Notification_ID INT IDENTITY(1,1) PRIMARY KEY, -- Auto-incrementing primary key User_ID INT NOT NULL FOREIGN KEY REFERENCES Users(User_ID),
Message VARCHAR(500) NOT NULL,
Type VARCHAR(20) CHECK (Type IN ('Match', 'Contest', 'Payment', 'System')), Is_Read BIT DEFAULT 0, -- 0 = Unread, 1 = Read
Created_At DATETIME DEFAULT GETDATE()
);
Select * from Notifications

