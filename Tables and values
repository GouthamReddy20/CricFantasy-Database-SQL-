Select * from team CREATE TABLE Series (
Series_ID INT PRIMARY KEY,
Series_Name VARCHAR(100) NOT NULL ,
Host_Country VARCHAR(50) NOT NULL, Start_Date DATE NOT NULL, End_Date DATE NOT NULL,
Format VARCHAR(20) CHECK (Format IN ('Test', 'ODI', 'T20', 'World Cup', 'Championship')), Status VARCHAR(15) CHECK (Status IN ('Upcoming', 'Ongoing', 'Completed')), Number_of_Matches INT DEFAULT 0,
Participating_Teams VARCHAR(500) NOT NULL
);
Select * from Series


