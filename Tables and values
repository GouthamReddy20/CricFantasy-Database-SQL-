CREATE TABLE Contest ( Contest_ID INT primary key,
Contest_Name VARCHAR(100) NOT NULL,
Match_ID VARCHAR(20) NOT NULL FOREIGN KEY REFERENCES Matches(Match_ID), Entry_Fee DECIMAL(8,2) DEFAULT 0.00,
Prize_Pool DECIMAL(10,2) NOT NULL,
Max_Participants INT DEFAULT 100, Current_Participants INT DEFAULT 0,
Contest_Type VARCHAR(20) CHECK (Contest_Type IN ('Free', 'Paid', 'Head-to-Head', 'Mega')), Start_Time DATETIME NOT NULL,
Status VARCHAR(20) DEFAULT 'Open' CHECK (Status IN ('Open', 'Closed', 'Completed')), Created_At DATETIME DEFAULT GETDATE()
);
Select * from Contest


