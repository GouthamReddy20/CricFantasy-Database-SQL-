CREATE TABLE ScoringRules ( Rule_ID INT PRIMARY KEY,
Action VARCHAR(50) UNIQUE NOT NULL, Points INT NOT NULL,
Is_Active BIT DEFAULT 1, Valid_From DATE, Valid_To DATE
);
Select * from ScoringRules

