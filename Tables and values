Select * from PlayerStats CREATE TABLE Rankings (
Format VARCHAR(20) CHECK (Format IN ('Test', 'ODI', 'T20','IPL')),
Player_Role VARCHAR(30) CHECK (Player_Role IN ('Batsman', 'Bowler', 'All-rounder')), Current_Rank INT NOT NULL,
Player_ID INT NOT NULL FOREIGN KEY REFERENCES Player(Player_ID), Player_Name VARCHAR(100) NOT NULL,
Team_ID INT NOT NULL FOREIGN KEY REFERENCES Team(Team_ID), Team_Name VARCHAR(50) NOT NULL,
Ranking_Points INT NOT NULL,
PRIMARY KEY (Format, Player_Role, Current_Rank)
);
