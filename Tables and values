Select * from Umpire CREATE TABLE Venue (
Venue_ID INT PRIMARY KEY,
Venue_Name VARCHAR(100) unique NOT NULL, City VARCHAR(50) NOT NULL,
Country VARCHAR(50) NOT NULL,
Capacity INT NOT NULL
);


