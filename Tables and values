Create table Umpire ( Umpire_ID INT PRIMARY KEY ,
Umpire_Name VARCHAR(50) unique NOT NULL, Country VARCHAR(50) NOT NULL,
Date_of_Birth DATE, Matches_Officiated INT DEFAULT 0
);
Select * from umpire

