CREATE TABLE Leaderboard (
Contest_ID INT NOT NULL FOREIGN KEY REFERENCES Contest(Contest_ID), User_ID INT NOT NULL FOREIGN KEY REFERENCES Users(User_ID),
User_Name VARCHAR(50) NOT NULL, -- Added for easier tracking
Team_ID INT NOT NULL FOREIGN KEY REFERENCES UserTeam(Team_ID), -- Added to link to the user's

team




);


Total_Points INT DEFAULT 0, Current_Rank INT,
Last_Updated DATETIME DEFAULT GETDATE(),
PRIMARY KEY (Contest_ID, User_ID)

Select * from Leaderboard
