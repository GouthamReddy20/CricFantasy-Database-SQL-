Select * from Matches CREATE TABLE Player (
Player_ID INT PRIMARY KEY, Player_Name VARCHAR(100) NOT NULL, DOB DATE,
Player_Type VARCHAR(20) CHECK (Player_Type IN ('Batsman', 'Bowler', 'All-rounder', 'Wicketkeeper')),
Batting_Style VARCHAR(50), Bowling_Style VARCHAR(50),
Team_ID INT foreign key references Team (Team_ID) NOT NULL, Team_Name VARCHAR(50) NOT NULL
);
Select * From Player

